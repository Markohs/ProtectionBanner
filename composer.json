{
    "name": "markohs/protectionbanner",
    "description": "Customizable Laravel Middleware to comply with EU cookie lawls and protect adult websites from child access forcing the client to accept conditions before any cookie or anything is sent.",
    "license": "MIT",
    "authors": [
        {
            "name": "Markohs",
            "email": "marcos@tyrellcorporation.es",
            "homepage": "https://github.com/Markohs"
        }
    ],
    "homepage": "https://github.com/markohs/protectionbanner",
    "keywords": ["Laravel", "ProtectionBanner","cookies","middleware","adult"],
    "require": {
        "jaybizzle/laravel-crawler-detect": "1.*",
        "illuminate/support": "~5|~6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Markohs\\ProtectionBanner\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Markohs\\ProtectionBanner\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Markohs\\ProtectionBanner\\ProtectionBannerServiceProvider"
            ],
            "aliases": {
                "ProtectionBanner": "Markohs\\ProtectionBanner\\Facades\\ProtectionBanner"
            }
        }
    }
}
